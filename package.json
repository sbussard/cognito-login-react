    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^4.1.2",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.1.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "uuid": "^3.0.1",
    "webpack": "^3.5.4",
    "webpack-dashboard": "^0.4.0",
