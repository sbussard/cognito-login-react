{
  "name": "cognito-login-react",
  "version": "1.0.0",
  "description": "a self explanatory sample app",
  "scripts": {
    "build": "webpack",
    "start": "NODE_ENV=development webpack-dashboard -c cyan -- webpack-dev-server --port 7000 --content-base build/",
    "test": "jest",
    "test-watch": "jest --watch",
    "analyze": "parallelshell 'npm test' 'npm run lint' 'flow'",
    "lint": "parallelshell 'npm run lint:styles' 'npm run lint:scripts'",
    "lint:styles": "sass-lint src/**/*.scss -v -q",
    "lint:scripts": "eslint {*.js,*.jsx,**/*.js,**/*.jsx}",
    "precommit": "npm run analyze"
  },
  "devDependencies": {
    "amazon-cognito-identity-js": "^1.10.0",
    "aws-sdk": "^2.7.19",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-root-import": "^4.1.5",
    "classnames": "^2.2.5",
    "css-loader": "0.23.1",
    "css-module-flow": "^1.0.0",
    "eslint": "^3.12.2",
    "eslint-plugin-flowtype": "^2.29.2",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.1",
    "html-webpack-plugin": "1.6.1",
    "husky": "^0.12.0",
    "immutable": "^3.8.1",
    "jest": "^18.0.0",
    "json-loader": "^0.5.4",
    "moment": "^2.17.1",
    "node-sass": "^3.13.0",
    "parallelshell": "^2.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sass-lint": "^1.10.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "uuid": "^3.0.1",
    "webpack": "^1.12.13",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "1.14.1"
  },
  "author": "Stephen Bussard",
  "license": "UNLICENSED",
  "jest": {
    "verbose": true,
    "testRegex": "(/spec/.*|(\\.|/)(spec))\\.jsx?$",
    "notify": true
  }
